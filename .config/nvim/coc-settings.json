{
    "languageserver": {
        "csharp-ls": {
          "command": "csharp-ls",
          "filetypes": ["cs"],
          "rootPatterns": ["*.csproj", ".vim/", ".git/", ".hg/"]
        },
        "clangd" :{
            "command": "/usr/bin/clangd",
            "filetypes": ["c", "cpp"],
            "rootPatterns": [".git/", ".vim/"],
            "args": []
        }
    }
}
